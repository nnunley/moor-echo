// Test optional parameters
fn {x, ?y=10} x + y endfn

// Test calling with both args
let f1 = fn {x, ?y=10} x + y endfn
f1(5, 3)

// Test calling with default
f1(5)

// Test rest parameters  
fn {x, @rest} rest endfn

// Test rest param collection
let f2 = fn {x, @rest} rest endfn
f2(1, 2, 3, 4, 5)

// Test mixed params
fn {x, ?y=10, @rest} [x, y, rest] endfn

let f3 = fn {x, ?y=10, @rest} [x, y, rest] endfn
f3(1)
f3(1, 2)
f3(1, 2, 3, 4, 5)