object EventTest
    property name = "EventTest"
    property event_count = 0
    
    event player_moved
        this.event_count = this.event_count + 1
        print("Player moved!")
    endevent
    
    event startup
        print(this.name + " initialized")
    endevent
    
    verb test_events
        emit player_moved
        emit startup
        return this.event_count
    endverb
endobject