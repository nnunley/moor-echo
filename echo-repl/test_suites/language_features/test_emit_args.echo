// Test emit with arguments
print("Testing emit with arguments");

// Emit without arguments
emit startup;

// Emit with single argument
emit player_moved("north");

// Emit with multiple arguments
emit damage_taken(25, "goblin");

// Emit with expression arguments
let damage = 10 + 5;
let attacker = "orc";
emit damage_taken(damage, attacker);

// Emit with complex arguments
emit game_event("level_up", {level: 5, xp: 1000});

print("Done testing emit");